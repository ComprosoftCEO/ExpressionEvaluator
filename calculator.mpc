project (calculator) {
  exename = calculator
  includes += Array Stack Queue
  install = .
 
  specific (make, gnuace) {
    compile_flags += -std=c++11 -Wall
  }

  Source_Files {
    driver.cpp
  }

  Header_Files {
    Array/Array_Base.h
    Array/Array.h
    Array/Fixed_Array.h
    Stack/Stack.h
    Queue/Queue.h
  }

  Template_Files {
    Array/Array_Base.cpp
    Array/Array.cpp
    Array/Fixed_Array.cpp
    Stack/Stack.cpp
    Queue/Queue.cpp
  }
}
